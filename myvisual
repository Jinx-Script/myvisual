-- Helper function to create sidebar tab
function UI:CreateTab(name, icon)
    local tab = Instance.new("TextButton")
    tab.Size = UDim2.new(1, 0, 0, 28) -- was 45
    // ... existing code ...
    local tabIcon = Instance.new("TextLabel")
    tabIcon.Size = UDim2.new(0, 14, 0, 14) -- was 20x20
    tabIcon.Position = UDim2.new(0, 7, 0.5, -7)
    tabIcon.TextSize = 11 -- was 16
    // ... existing code ...
    local tabLabel = Instance.new("TextLabel")
    tabLabel.Size = UDim2.new(1, -26, 1, 0) -- was -45
    tabLabel.Position = UDim2.new(0, 26, 0, 0)
    tabLabel.TextSize = 10 -- was 14
    // ... existing code ...
    local tabContentList = Instance.new("UIListLayout")
    tabContentList.Padding = UDim.new(0, 7) -- was 15
    // ... existing code ...
    -- Special layout for Pet tab - split into three panels
    if name == "Pet" then
        -- Left panel for ESP settings
        local leftPanel = Instance.new("ScrollingFrame")
        leftPanel.Size = UDim2.new(0.32, 0, 1, 0)
        leftPanel.ScrollBarThickness = 4 -- was 6
        local leftList = Instance.new("UIListLayout")
        leftList.Padding = UDim.new(0, 5) -- was 10
        local leftPadding = Instance.new("UIPadding")
        leftPadding.PaddingTop = UDim.new(0, 5) -- was 10
        leftPadding.PaddingLeft = UDim.new(0, 5)
        leftPadding.PaddingRight = UDim.new(0, 5)
        leftPadding.PaddingBottom = UDim.new(0, 5)
        // ... existing code ...
        local middlePanel = Instance.new("ScrollingFrame")
        middlePanel.Size = UDim2.new(0.32, 0, 1, 0)
        middlePanel.Position = UDim2.new(0.34, 0, 0, 0)
        middlePanel.ScrollBarThickness = 4
        local middleList = Instance.new("UIListLayout")
        middleList.Padding = UDim.new(0, 5)
        local middlePadding = Instance.new("UIPadding")
        middlePadding.PaddingTop = UDim.new(0, 5)
        middlePadding.PaddingLeft = UDim.new(0, 5)
        middlePadding.PaddingRight = UDim.new(0, 5)
        middlePadding.PaddingBottom = UDim.new(0, 5)
        // ... existing code ...
        local refreshButton = Instance.new("TextButton")
        refreshButton.Size = UDim2.new(1, 0, 0, 24) -- was 40
        refreshButton.TextSize = 10 -- was 14
        // ... existing code ...
        local rightPanel = Instance.new("ScrollingFrame")
        rightPanel.Size = UDim2.new(1, 0, 1, -30) -- was -50
        rightPanel.Position = UDim2.new(0, 0, 0, 30) -- was 50
        rightPanel.ScrollBarThickness = 4
        local rightList = Instance.new("UIListLayout")
        rightList.Padding = UDim.new(0, 4) -- was 8
        local rightPadding = Instance.new("UIPadding")
        rightPadding.PaddingTop = UDim.new(0, 5)
        rightPadding.PaddingLeft = UDim.new(0, 5)
        rightPadding.PaddingRight = UDim.new(0, 5)
        rightPadding.PaddingBottom = UDim.new(0, 5)
        // ... existing code ...
    end
// ... existing code ...
-- Helper function to create toggle
function UI:CreateToggle(tabName, text, enabled, callback, panelType)
    // ... existing code ...
    local toggle = Instance.new("Frame")
    toggle.Size = UDim2.new(1, 0, 0, 24) -- was 40
    // ... existing code ...
    local toggleLabel = Instance.new("TextLabel")
    toggleLabel.Size = UDim2.new(1, -36, 1, 0) -- was -60
    toggleLabel.Position = UDim2.new(0, 8, 0, 0) -- was 15
    toggleLabel.TextSize = 9 -- was 13
    // ... existing code ...
    local toggleSwitch = Instance.new("TextButton")
    toggleSwitch.Size = UDim2.new(0, 24, 0, 12) -- was 40x20
    toggleSwitch.Position = UDim2.new(1, -28, 0.5, -6)
    // ... existing code ...
    local switchIndicator = Instance.new("Frame")
    switchIndicator.Size = UDim2.new(0, 8, 0, 8) -- was 16x16
    switchIndicator.Position = enabled and UDim2.new(1, -10, 0.5, -4) or UDim2.new(0, 1, 0.5, -4)
    // ... existing code ...
