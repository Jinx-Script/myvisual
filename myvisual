-- Helper function to create slider
function UI:CreateSlider(tabName, text, min, max, default, callback, panelType)
    // ... existing code ...
    local slider = Instance.new("Frame")
    slider.Size = UDim2.new(1, 0, 0, 32) -- was 60
    // ... existing code ...
    local sliderLabel = Instance.new("TextLabel")
    sliderLabel.Size = UDim2.new(1, -36, 0, 12) -- was -60, 20
    sliderLabel.Position = UDim2.new(0, 8, 0, 4) -- was 20, 10
    sliderLabel.TextSize = 9 -- was 14
    // ... existing code ...
    local sliderValue = Instance.new("TextLabel")
    sliderValue.Size = UDim2.new(0, 24, 0, 12) -- was 40x20
    sliderValue.Position = UDim2.new(1, -32, 0, 4)
    sliderValue.TextSize = 9 -- was 14
    // ... existing code ...
    local sliderTrack = Instance.new("Frame")
    sliderTrack.Size = UDim2.new(1, -24, 0, 2) -- was -40, 4
    sliderTrack.Position = UDim2.new(0, 8, 0, 20) -- was 20, 40
    // ... existing code ...
    local sliderFill = Instance.new("Frame")
    // ... existing code ...
    local sliderButton = Instance.new("TextButton")
    sliderButton.Size = UDim2.new(0, 8, 0, 8) -- was 16x16
    sliderButton.Position = UDim2.new((default - min) / (max - min), -4, 0.5, -4)
    // ... existing code ...
-- Helper function to create textbox
