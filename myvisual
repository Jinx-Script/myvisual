    textbox.Position = UDim2.new(0, 8, 0, 18) -- was 20, 30
    textbox.TextSize = 9 -- was 12
    // ... existing code ...
-- Helper function to create dropdown
function UI:CreateDropdown(tabName, text, options, defaultIndex, callback, panelType)
    // ... existing code ...
    local dropdown = Instance.new("Frame")
    dropdown.Size = UDim2.new(1, 0, 0, 32) -- was 60
    // ... existing code ...
    local dropdownLabel = Instance.new("TextLabel")
    dropdownLabel.Size = UDim2.new(1, -10, 0, 12) -- was -20, 20
    dropdownLabel.Position = UDim2.new(0, 8, 0, 4)
    dropdownLabel.TextSize = 9 -- was 14
    // ... existing code ...
    local dropdownButton = Instance.new("TextButton")
    dropdownButton.Size = UDim2.new(1, -20, 0, 14) -- was -40, 25
    dropdownButton.Position = UDim2.new(0, 8, 0, 18)
    dropdownButton.TextSize = 9 -- was 12
    // ... existing code ...
    local dropdownList = Instance.new("Frame")
    dropdownList.Size = UDim2.new(1, -20, 0, #options * 14) -- was -40, 25
    dropdownList.Position = UDim2.new(0, 8, 0, 32) -- was 20, 55
    // ... existing code ...
    for i, option in ipairs(options) do
        local optionButton = Instance.new("TextButton")
        optionButton.Size = UDim2.new(1, 0, 0, 12) -- was 25
        optionButton.TextSize = 9 -- was 12
        // ... existing code ...
    end
// ... existing code ...
